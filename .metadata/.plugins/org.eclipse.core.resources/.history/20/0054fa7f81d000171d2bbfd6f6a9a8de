import java.util.ArrayList;

public class ApprovalService {
	public boolean isApproved(long studentId) {
		SqlProvider sql = new SqlProvider();
		countOfExams = sql.LoadCountOfExams(studentId);
		if (grades.size() > 0) {
			double avg = 0.;
			for (int i = 0; i < grades.size(); ++i) {
				avg += grades.get(i);
			}
			avg /= grades.size();
		}
		else {
			return false;
		}
		return false;
	}	
}
